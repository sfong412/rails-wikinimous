<div class="navbar bg-light">
  <h2><%= link_to 'Wikinimous', articles_path %></h2>
  <%= link_to 'New Article', new_article_path %>
</div>

<div class="container">
  <ul class="list">
  <% @articles.each do |article| %>
  <li>
    <h1><%= link_to article.title, article_path(article) %></h1>
    <p><%= article.content %></p>
  </li>
  <% end %>
  </ul>
</div>
